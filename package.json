{
  "name": "stats-node",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "prepush": "yarn run lint",
    "prettier": "prettier \"**/*\" --list-different --ignore-unknown",
    "prettier:fix": "prettier \"**/*\" --write --ignore-unknown",
    "lint": "eslint . --ext .ts && yarn run prettier",
    "lint:fix": "eslint . --ext .ts --fix && yarn run prettier:fix",
    "migrate": "ts-node migrate.ts",
    "start:prod": "yarn build && node dist/src/index.js",
    "start": "ts-node .",
    "docker": "docker-compose up -d",
    "docker:import": "docker exec mongodb mongorestore",
    "pre:deploy": "yarn install && yarn run migrate && yarn run build",
    "pm2:setup": "yarn run pre:deploy && pm2 start pm2.json",
    "pm2": "yarn run pre:deploy && pm2 restart stats-node",
    "build": "tsc"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "axios": "^0.18.0",
    "ip6addr": "^0.2.3",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-ctx-paginate": "^0.0.4",
    "koa-morgan": "^1.0.1",
    "koa-pagination": "^4.0.0",
    "koa-passport": "^4.1.3",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.4.0",
    "locutus": "^2.0.11",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "mongoose": "^4.12.5",
    "mongoose-paginate": "^5.0.3",
    "passport-google-oauth20": "^2.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-qs": "^2.0.0",
    "@types/koa-router": "^7.0.37",
    "@types/locutus": "^0.0.5",
    "@types/lodash": "^4.14.150",
    "@types/mongoose": "^4.0.0",
    "@types/mongoose-paginate": "^5.0.8",
    "@types/node": "^14.14.7",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.14.0",
    "husky": "^1.2.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
