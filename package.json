{
  "name": "stats-node",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "prepush": "yarn run lint",
    "prettier": "prettier \"**/*\" --list-different --ignore-unknown",
    "prettier:fix": "prettier \"**/*\" --write --ignore-unknown",
    "lint": "eslint . && yarn run prettier && yarn tsc --project tsconfig.eslint.json",
    "lint:fix": "eslint . --fix && yarn run prettier:fix",
    "migrate": "node --require ts-node/register migrate.ts",
    "start:prod": "yarn run migrate && node dist/index.js",
    "start": "ts-node src/index.ts",
    "docker:import": "docker exec stats_node_db mongorestore",
    "pre:deploy": "yarn install && yarn run build && yarn run migrate",
    "pm2:setup": "yarn run pre:deploy && pm2 start pm2.json",
    "pm2": "yarn run pre:deploy && pm2 restart stats-node",
    "build": "tsc"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "axios": "^0.21.1",
    "cls-hooked": "^4.2.2",
    "convict": "^6.0.0",
    "google-auth-library": "^9.15.0",
    "googleapis": "^144.0.0",
    "ip6addr": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-ctx-paginate": "^0.0.6",
    "koa-morgan": "^1.0.1",
    "koa-qs": "^3.0.0",
    "koa-router": "^10.0.0",
    "koa-session": "^6.2.0",
    "locutus": "^2.0.11",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "mongoose": "^6.0.12",
    "mongoose-paginate": "^5.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.3",
    "@types/convict": "^5.2.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.1",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-qs": "^2.0.0",
    "@types/koa-router": "^7.0.37",
    "@types/koa-session": "^5.10.3",
    "@types/locutus": "^0.0.6",
    "@types/lodash": "^4.14.150",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-paginate": "^5.0.10",
    "@types/node": "^14.14.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.14.0",
    "husky": "^1.2.1",
    "prettier": "^2.2.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
